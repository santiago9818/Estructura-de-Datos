 private   String nombre;        
 public Estudiante( int codigo,String nombre)
 {
  this.codigo=codigo;
  this.nombre=nombre;
 }
public String toString()
{
    return ("el nombre es " + nombre +  ", con codigoÂ¨" + codigo);
}
 
}
